  <%= render :partial=>'admin/shared/partner_general_fields', :locals=>{:f=>f} %>

  <%= partner_rollover_button 'update', @partner.persisted? ? 'Update' : 'Create' %>

  <%= render :partial=>'admin/shared/partner_settings_fields', :locals=>{:f=>f} %>

